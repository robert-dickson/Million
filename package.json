{
  "name": "million",
  "version": "1.8.9-0",
  "description": "<1kb compiler-augmented virtual DOM. It's fast!",
  "main": "dist/million.mjs",
  "module": "dist/million.mjs",
  "jsdelivr": "dist/million.mjs",
  "unpkg": "dist/million.mjs",
  "types": "dist/million.d.ts",
  "scripts": {
    "dev": "zx scripts/dev.mjs",
    "build": "zx scripts/build.mjs",
    "lint": "eslint packages/**",
    "cleanup": "prettier --write packages",
    "test": "vitest --coverage",
    "bench": "vite --config benchmarks/vite.config.ts",
    "release": "zx scripts/release.mjs",
    "bump": "np && zx scripts/citation.mjs",
    "welcome": "zx scripts/welcome.mjs",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidenybai/million.git"
  },
  "files": [
    "dist/**/*",
    "/*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/million.mjs",
      "require": "./dist/million.cjs",
      "types": "./dist/million.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.cjs",
      "types": "./jsx-runtime.d.ts"
    },
    "./ssr": {
      "import": "./dist/ssr.mjs",
      "require": "./dist/ssr.cjs",
      "types": "./ssr.d.ts"
    },
    "./block": {
      "import": "./dist/block.mjs",
      "require": "./dist/block.cjs",
      "types": "./block.d.ts"
    },
    "./router": {
      "import": "./dist/router.mjs",
      "require": "./dist/router.cjs",
      "types": "./router.d.ts"
    },
    "./html": {
      "import": "./dist/html.mjs",
      "require": "./dist/html.cjs",
      "types": "./html.d.ts"
    },
    "./morph": {
      "import": "./dist/morph.mjs",
      "require": "./dist/morph.cjs",
      "types": "./morph.d.ts"
    },
    "./vite-plugin-million": {
      "import": "./dist/vite-plugin-million.mjs",
      "require": "./dist/vite-plugin-million.cjs",
      "types": "./vite-plugin-million.d.ts"
    },
    "./shared": {
      "import": "./dist/shared.mjs",
      "require": "./dist/shared.cjs",
      "types": "./shared.d.ts"
    }
  },
  "sideEffects": false,
  "keywords": [
    "million",
    "virtualdom",
    "vdom",
    "library",
    "web",
    "ui",
    "browser"
  ],
  "funding": "https://github.com/sponsors/aidenybai",
  "author": {
    "name": "Aiden Bai",
    "email": "hello@aidenybai.com",
    "url": "https://aidenybai.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aidenybai/million/issues"
  },
  "homepage": "https://github.com/aidenybai/million",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "zx scripts/check.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-react-constant-elements": "^7.17.6",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@picocss/pico": "^1.5.0",
    "@types/babel__core": "^7.1.19",
    "@types/benchmark": "^2.1.1",
    "@types/canvas-confetti": "^1.4.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/virtual-dom": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitest/ui": "^0.10.2",
    "benchmark": "^2.1.4",
    "c8": "^7.11.2",
    "canvas-confetti": "^1.5.1",
    "chart.js": "^3.7.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "export-size": "^0.5.2",
    "fsxx": "^0.0.5",
    "htm": "^3.1.1",
    "hundred": "^0.1.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "lodash": "^4.17.21",
    "np": "^7.6.1",
    "ora": "^6.1.0",
    "prettier": "^2.6.2",
    "recast": "^0.21.1",
    "simple-git": "^3.7.1",
    "simple-virtual-dom": "^0.1.10",
    "snabbdom": "^3.5.0",
    "source-map": "^0.7.3",
    "taze": "^0.6.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "virtual-dom": "^2.1.1",
    "vite": "^2.9.7",
    "vitest": "^0.10.2",
    "zx": "^6.1.0"
  }
}
